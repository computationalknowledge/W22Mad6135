// 3 ways to declare variables

let x = 1; 
let y = 1;

for ( ; y<=10 ; y++ ){
    for ( ; x<=10 ; x++ )   {
        console.log("x is ", x);
        console.log("y is ", y);
        console.log(x*y);
    }
    x=1;
}


